{
  "version": 2,
  "builds": [
    {
      "src": "Deforestation-App/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Deforestation-App/index.html"
    },
    {
      "src": "/about",
      "dest": "/Deforestation-App/about.html"
    },
    {
      "src": "/app", 
      "dest": "/Deforestation-App/app.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Deforestation-App/index.html"
    }
  ]
}